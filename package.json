{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "format": "prettier 'resources/**/*.{css,js,json,css,ts,tsx}' '*.{js,css}' --write",
        "type-check": "tsc --noEmit",
        "prepare": "husky install"
    },
    "devDependencies": {
        "flatpickr": "^4.6.3",
        "husky": "^7.0.0",
        "laravel-mix": "^6.0.43",
        "lint-staged": "^9.5.0",
        "postcss": "^8.4.13",
        "postcss-easy-import": "^4.0.0",
        "prettier": "^1.18.2",
        "tailwindcss": "^3.0.24"
    },
    "dependencies": {
        "@alpinejs/focus": "^3.10.2",
        "@swup/a11y-plugin": "^2.0.0",
        "@swup/livewire-plugin": "^1.0.0",
        "@swup/preload-plugin": "^1.0.3",
        "@yaireo/tagify": "^4.12.0",
        "alpinejs": "^3.10",
        "swup": "^2.0.14"
    }
